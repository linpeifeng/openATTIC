{
  "env": {
    "browser": true,
    "jquery": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "_": false,
    "angular": false,
    "EventSource": false,
    "globalConfig": false
  },
  "rules": {
    "no-bitwise": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-extend-native": 2,
    "guard-for-in": 2,
    "block-scoped-var": 2,
    "no-use-before-define": 0,
    "max-params": [
      2,
      15
    ],
    "no-caller": 2,
    "no-irregular-whitespace": 2,
    "no-new": 0,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-var": 0,
    "no-empty": [
      2,
      {
        "allowEmptyCatch": true
      }
    ],
    "no-implicit-coercion": [
      2,
      {
        "boolean": true,
        "string": true,
        "number": true
      }
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": false
      }
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [
      2,
      { "max": 1 }
    ],
    "no-multi-str": 2,
    "one-var": [
      2,
      "never"
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "no-spaced-func": 2,
    "space-before-function-paren": [
      2,
      "always"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "no-trailing-spaces": 2,
    "max-len": [
      2,
      120
    ],
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "operator-linebreak": [
      2,
      "after"
    ],
    "wrap-iife": 2,
    "space-infix-ops": 2,
    "keyword-spacing": [
      2,
      {
        "overrides": {
          "else": {
            "before": true
          },
          "while": {
            "before": true
          },
          "catch": {
            "before": true
          },
          "finally": {
            "before": true
          }
        }
      }
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1,
        "FunctionDeclaration": {
          "body": 1,
          "parameters": 2
        },
        "FunctionExpression": {
          "body": 1,
          "parameters": 2
        }
      }
    ],
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ],
    "new-cap": [
      2,
      {
        "capIsNewExceptions": []
      }
    ],
    "one-var-declaration-per-line": [
      2,
      "always"
    ],
    "no-multi-spaces": ["error", { "exceptions": { "Property": true } }],
    "arrow-spacing": "error",
    "semi": ["error", "always"],
    "no-extra-semi": "error"
  }
}
